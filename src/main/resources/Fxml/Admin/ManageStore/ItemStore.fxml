<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.app.Controller.Admin.ManageStore.ItemStoreController"
            prefHeight="30.0" prefWidth="814.0">
    <children>
        <HBox fx:id="hbox" layoutX="-1.0" prefHeight="30.0" prefWidth="814.0">
            <children>
                <AnchorPane prefHeight="30.0" prefWidth="814.0">
                    <children>
                        <Label fx:id="storeId" layoutX="33.0" layoutY="27.0" style="-fx-font-size: 16px; -fx-font-weight: 700;" text="Mã kho" />
                        <Label fx:id="name" layoutX="103.0" layoutY="27.0" style="-fx-font-size: 16px; -fx-font-weight: 700;" text="Tên" />
                        <Label fx:id="address" layoutX="425.0" layoutY="28.0" style="-fx-font-size: 16px; -fx-font-weight: 700;" text="Địa chỉ" />
                        <Button fx:id="update_btn" layoutX="754.0" layoutY="27.0" mnemonicParsing="false" style="-fx-background-color: ABC9FF; -fx-text-fill: #ffffff;" text="Sửa" />
                        <Button fx:id="remove_btn" layoutX="799.0" layoutY="27.0" mnemonicParsing="false" style="-fx-background-color: FF8B8B; -fx-text-fill: #ffffff;" text="Xóa" />
                    </children>
                </AnchorPane>
            </children>
        </HBox>

    </children>
</AnchorPane>
